    "email": "673115816@qq.com"
  },
  "scripts": {
    "preinstall": "node ./scripts/preinstall.js",
    "ncu:all": "pnpm -r exec ncu -u",
    "build": "pnpm -r build",
    "update": "pnpm -r up --latest",
    "clean": "pnpm -r clean",
    "rm": "rm -rf node_modules && pnpm -r exec rm -rf node_modules",
    "view": "pnpm -rc exec pnpm view $PNPM_PACKAGE_NAME",
    "web:dev": "npm run start -C packages/web",
    "web:build": "pnpm -r build --filter @wyy/web",
    "start": "pnpm --parallel -r start",
    "commit": "git-cz"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "2.0.1",
    "@vitest/ui": "^0.23.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.5",
    "eslint-plugin-testing-library": "^5.6.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "*",
    "react-test-renderer": "18.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "*",
    "vitest": "^0.23.0"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
