{
  "name": "wyy",
  "version": "1.0.0",
  "description": "React music player",
  "keywords": [
    "react"
  ],
  "main": "src/client.tsx",
  "homepage": "https://neteasecloudmusic.fun",
  "bugs": {
    "url": "https://github.com/q673115816/react-wyy-music-pc/issues",
