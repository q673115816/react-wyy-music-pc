{
  "name": "wyy",
  "version": "1.0.0",
  "description": "React music player",
  "keywords": [
    "react"
  ],
  "main": "src/client.tsx",
  "homepage": "https://neteasecloudmusic.fun",
  "bugs": {
    "url": "https://github.com/q673115816/react-wyy-music-pc/issues",
    "email": "673115816@qq.com"
  },
  "license": "APGL",
  "author": {
    "name": "linjinjun",
    "email": "673115816@qq.com"
  },
  "scripts": {
    "upgrade:pkg": "pnpm -r exec ncu -u",
    "build": "pnpm -r run build",
    "update": "pnpm -r up --latest",
    "clean": "rm -rf node_modules **/*/node_modules",
    "view": "pnpm -rc exec pnpm view $PNPM_PACKAGE_NAME",
    "web:build": "pnpm -r run build --filter @wyy/web",
    "web:start": "pnpm -r run start --filter @wyy/web",
    "start": "concurrently --kill-others \"npm run start -C packages/web\" \"npm run start -C packages/pics-viewer\"",
    "commit": "git-cz",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "babel-jest": "^28.1.2",
    "concurrently": "7.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-testing-library": "^5.5.1",
    "fs-extra": "^10.1.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "*",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^28.0.5",
    "typescript": "*"
  },
  "engines": {
    "node": ">=10",
    "pnpm": ">=3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
